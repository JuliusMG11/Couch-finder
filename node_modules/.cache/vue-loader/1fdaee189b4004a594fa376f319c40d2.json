eer</label>\r\n        </span>\r\n    </base-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    emits: ['change-filter'],\r\n    data() {\r\n        return {\r\n            filters: {\r\n                frontend: true,\r\n                backend: true,\r\n                career: true\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        setFilter(event) {\r\n            const inputId = event.target.id;\r\n            const isActive = event.target.checked;\r\n            const updatedFilters = {\r\n                ...this.filters,\r\n                [inputId]: isActive\r\n            };\r\n\r\n            this.filters = updatedFilters;\r\n            this.$emit('change-filter', updatedFilters);\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\nh2 {\r\n  margin: 0.5rem 0;\r\n}\r\n\r\n.filter-option {\r\n  margin-right: 1rem;\r\n}\r\n\r\n.filter-option label,\r\n.filter-option input {\r\n  vertical-align: middle;\r\n}\r\n\r\n.filter-option label {\r\n  margin-left: 0.25rem;\r\n}\r\n\r\n.filter-option.active label {\r\n  font-weight: bold;\r\n}\r\n</style>"]}]}